{
  "name": "resume",
  "version": "1.0.0",
  "description": "personal resume written in latex",
  "main": "index.js",
  "scripts": {
    "start": "lite-server",
    "build": "npm run clean && npm run compile",
    "compile": "relaxed --build-once src/resume.html && mv src/resume.pdf . && rm -rf src/*.htm",
    "clean": "rm -rf *.pdf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanye24/resume.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seanye24/resume/issues"
  },
  "homepage": "https://github.com/seanye24/resume#readme",
  "dependencies": {
    "lite-server": "^2.6.1",
    "relaxedjs": "^0.2.4"
  },
  "devDependencies": {
    "eslint": "^8.15.0"
  }
}
